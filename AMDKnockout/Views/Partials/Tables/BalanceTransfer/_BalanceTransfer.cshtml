<table data-bind="page: {id: 'BalanceTransfer', with: balancetransfer}" class="table table-hover">
    <thead>
        <tr class="table-row">
            <th class="col-large"></th>
            <th class="col-medium" data-bind="click: App.BalanceTransferController.Table.sortByBalanceTransfer">
                <a href="#">Balance Transfers</a>
            </th>
            <th class="col-small" data-bind="click: App.BalanceTransferController.Table.sortByBalanceTransferFee">
                <a href="#">Transfer Fee</a>
            </th>
            <th class="col-small" data-bind="click: App.BalanceTransferController.Table.sortBySavings">
                <a href="#">Savings</a>
            </th>
            <th class="col-small" data-bind="click: App.BalanceTransferController.Table.sortByEligibility">
                <a href="#">Eligibility</a>
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody data-bind="foreach: cards">
        @Html.Partial(Url.Content("~/Views/Partials/Tables/BalanceTransfer/_TableRow.cshtml"))
    </tbody>
</table>

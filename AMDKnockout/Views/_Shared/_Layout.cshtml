@model AMDKnockout.Models.ResponseData

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="en">
<head>
    <base href="/" />
    <link href="https://fonts.googleapis.com/css?family=Copse" rel="stylesheet" type="text/css">
    <link href="@Url.Content("~/Public/stylesheets/screen.css")" media="screen, projection" rel="stylesheet" type="text/css" />

    <script data-main="@Url.Content("~/Public/script/main.js")" src="@Url.Content("~/Public/script/require-jquery.js")"></script>
</head>
<body>
    <article class="main col-12">
        <header class="header cf container">
            <div class="col-3">
                <img src="https://s3-eu-west-1.amazonaws.com/tm-header/images/TotallyMoney_Logo.png" alt="logo" width="206" height="54">
            </div>
        </header>
        
        <section class="col-12 cf main-feature">
            <div class="container">
                <div class="main-feature-left col-12">
                    <h1 class="feature">The UKs Top Credit Cards</h1>
                    <h3 class="feature">We compare over 200 cards from 25 providers including exclusive offers not available anywhere else</h3>
                </div>
            </div>
        </section>
    
        <section class="container centered gutted">
            @RenderBody()
        </section>
    </article>
    
</body>
<script type="text/javascript">
    window.ResponseData = @Html.Raw(Model.Data)
</script>
</html>
